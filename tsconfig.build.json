{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"declaration": true,
		"emitDeclarationOnly": true,
		"declarationDir": "./build",
		"outDir": "./build",
		"noEmit": false,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"allowImportingTsExtensions": true,
		"strict": true
	},
	"include": ["src"]
}
